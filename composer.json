{
  "name": "lendable/gocardless-enterprise-bundle",
  "type": "symfony-bundle",
  "description": "integration of gocardless enterprise library into symfony",
  "keywords": [
    "gocardless",
    "Symfony2"
  ],
  "homepage": "https://github.com/Lendable/GocardlessEnterpriseBundle.git",
  "license": "MIT",
  "authors": [
    {
      "name": "The Lendable Team",
      "email": "contact@lendable.co.uk"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Lendable/guzzle-bundle.git"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "gocardless/enterprise": "~3.0",
    "symfony/symfony": "~2.1|~3.0",
    "lendable/guzzle-bundle": "~2.0",
    "stof/doctrine-extensions-bundle": "*"
  },
  "autoload": {
    "psr-0": {
      "Gocardless\\EnterpriseBundle": ""
    }
  },
  "target-dir": "Gocardless/EnterpriseBundle"
}
