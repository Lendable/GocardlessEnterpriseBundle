{
  "name": "lendable/gocardless-enterprise-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "integration of gocardless enterprise library into symfony",
  "keywords": ["gocardless", "symfony", "bundle", "lendable"],
  "authors": [
    {
      "name": "Lendable Ltd",
      "email": "dev@lendable.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Gocardless\\EnterpriseBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gocardless\\EnterpriseBundle\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Lendable/guzzle-bundle.git"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "gocardless/enterprise": "~3.0",
    "symfony/symfony": "~2.1|~3.0|~4.0",
    "lendable/guzzle-bundle": "~2.0",
    "stof/doctrine-extensions-bundle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "^1.0",
    "roave/security-advisories": "dev-master",
    "jakub-onderka/php-parallel-lint": "^0.9.2"
  }
}
